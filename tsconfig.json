{
  "compilerOptions": {
    "module": "ESNext",
    "target": "esnext",
    "lib": [ "es6", "esnext", "esnext.asynciterable", "dom", "es2018" ],
    "inlineSourceMap": false,
    "inlineSources": false,
    "baseUrl": "src",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  },
  "exclude": [
    "node_modules",
    "public",
    "data"
  ]
}
